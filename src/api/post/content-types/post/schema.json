{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "blocks",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
